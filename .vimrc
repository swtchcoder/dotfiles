set expandtab
set smartindent
set shiftwidth=2
set tabstop=2

set nu
set cursorline

set fillchars=eob:\ 

syntax on
filetype on

let g:seoul256_background = 234
colorscheme seoul256

inoremap " ""<left>
inoremap ' ''<left>
inoremap ` ``<left>
inoremap ( ()<left>
inoremap [ []<left>
inoremap { {}<left>
